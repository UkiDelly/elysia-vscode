{
  "name": "elysia-route-visualizer",
  "displayName": "ElysiaJS Route Visualizer",
  "description": "Visualizes ElysiaJS routes in VSCode sidebar.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:typescript"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "elysia-routes-container",
          "title": "Elysia Routes",
          "icon": "$(list-tree)"
        }
      ]
    },
    "views": {
      "elysia-routes-container": [
        {
          "id": "elysia-routes-view",
          "name": "Routes"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/node": "^18.0.0",
    "@types/mocha": "^10.0.1",
    "typescript": "^5.1.3",
    "ts-loader": "^9.4.3",
    "webpack": "^5.85.0",
    "webpack-cli": "^5.1.1",
    "eslint": "^8.41.0"
  }
}
